@model ProjectDb.Models.MeetingType



<form method="post" asp-action="CustomerMeetTypCreate">
    <div class="form-group">
        <label asp-for="MeetingTypeDescription" class="control-label">Görüşme Tipi</label>
        <input asp-for="MeetingTypeDescription" class="form-control" />
        <span asp-validation-for="MeetingTypeDescription" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Kaydet</button>
</form>


